{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "consent-under-tpo.json",
  "title": "Consent under TPO",
  "additionalProperties": true,
  "description": "An application that will affect a protected tree including those covered by a Tree Preservation Order (TPO) or those which grow in a conservation area",
  "type": "object",
  "properties": {
    "agent-contact": {
      "$ref": "#/definitions/agent-contact"
    },
    "agent-details": {
      "$ref": "#/definitions/agent-details"
    },
    "applicant-contact": {
      "$ref": "#/definitions/applicant-contact"
    },
    "applicant-details": {
      "$ref": "#/definitions/applicant-details"
    },
    "checklist": {
      "$ref": "#/definitions/checklist"
    },
    "conflict-of-interest": {
      "$ref": "#/definitions/conflict-of-interest"
    },
    "declaration": {
      "$ref": "#/definitions/declaration"
    },
    "tpo": {
      "$ref": "#/definitions/tpo"
    },
    "tree-work-details": {
      "$ref": "#/definitions/tree-work-details"
    },
    "trees-additional": {
      "$ref": "#/definitions/trees-additional"
    },
    "trees-ownership": {
      "$ref": "#/definitions/trees-ownership"
    },
    "application": {
      "$ref": "#/definitions/application"
    }
  },
  "required": [
    "agent-contact",
    "agent-details",
    "applicant-contact",
    "applicant-details",
    "checklist",
    "conflict-of-interest",
    "declaration",
    "tpo",
    "tree-work-details",
    "trees-additional",
    "trees-ownership",
    "application"
  ],
  "definitions": {
    "agent": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        },
        "company": {
          "type": "string"
        },
        "user-role": {
          "type": "string",
          "enum": [
            "agent",
            "proxy"
          ]
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "contact-details": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "phone-numbers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/phone-number"
          },
          "minItems": 1
        }
      },
      "required": [
        "email",
        "phone-numbers"
      ]
    },
    "applicant": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "person": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "first-name": {
          "type": "string"
        },
        "last-name": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        }
      },
      "required": [
        "first-name",
        "last-name",
        "address-text"
      ]
    },
    "phone-number": {
      "type": "object",
      "properties": {
        "number": {
          "type": "string"
        },
        "contact-priority": {
          "type": "string",
          "enum": [
            "primary",
            "secondary"
          ]
        }
      },
      "required": []
    },
    "supporting-document": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        }
      },
      "required": [
        "reference"
      ]
    },
    "tree-details": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "species": {
          "type": "string"
        },
        "description-of-works": {
          "type": "string"
        },
        "reason": {
          "type": "string"
        },
        "replanting-description": {
          "type": "string"
        }
      },
      "required": [
        "reference"
      ]
    },
    "owner": {
      "type": "object",
      "properties": {
        "person": {
          "$ref": "#/definitions/person"
        },
        "contact-details": {
          "$ref": "#/definitions/contact-details"
        }
      },
      "required": [
        "person"
      ]
    },
    "trees-additional": {
      "type": "object",
      "properties": {
        "advice-from-authority": {
          "type": "string"
        },
        "condition-concerns": {
          "type": "boolean"
        },
        "causing-subsidence": {
          "type": "boolean"
        },
        "causing-structural-damage": {
          "type": "boolean"
        },
        "supporting-documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/supporting-document"
          },
          "minItems": 1
        }
      },
      "required": [
        "condition-concerns",
        "causing-subsidence",
        "causing-structural-damage",
        "supporting-documents"
      ]
    },
    "trees-ownership": {
      "type": "object",
      "properties": {
        "is-applicant-owner": {
          "type": "boolean"
        },
        "owner": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/owner"
          },
          "minItems": 1
        }
      },
      "required": [
        "is-applicant-owner"
      ],
      "if": {
        "properties": {
          "is-applicant-owner": {
            "const": false
          }
        }
      },
      "then": {
        "required": [
          "owner"
        ]
      }
    },
    "conflict-of-interest": {
      "type": "object",
      "properties": {
        "conflict-to-declare": {
          "type": "boolean"
        },
        "conflict-person-name": {
          "type": "string"
        },
        "conflict-details": {
          "type": "string"
        }
      },
      "required": [
        "conflict-to-declare"
      ],
      "allOf": [
        {
          "if": {
            "properties": {
              "conflict-to-declare": {
                "const": true
              }
            }
          },
          "then": {
            "required": [
              "conflict-person-name"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "conflict-to-declare": {
                "const": true
              }
            }
          },
          "then": {
            "required": [
              "conflict-details"
            ]
          }
        }
      ]
    },
    "agent-contact": {
      "type": "object",
      "properties": {
        "agent-reference": {
          "type": "string"
        },
        "contact-details": {
          "$ref": "#/definitions/contact-details"
        }
      },
      "required": [
        "agent-reference",
        "contact-details"
      ]
    },
    "applicant-details": {
      "type": "object",
      "properties": {
        "applicants": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/applicant"
          },
          "minItems": 1
        }
      },
      "required": [
        "applicants"
      ]
    },
    "tpo": {
      "type": "object",
      "properties": {
        "tpo-reference": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "tpo-provided-by": {
          "type": "string",
          "enum": [
            "applicant",
            "system"
          ]
        }
      },
      "required": []
    },
    "declaration": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "declaration-confirmed": {
          "type": "boolean"
        },
        "declaration-date": {
          "type": "string"
        }
      },
      "required": [
        "name",
        "declaration-confirmed",
        "declaration-date"
      ]
    },
    "tree-work-details": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        },
        "tree-details": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/tree-details"
          },
          "minItems": 1
        }
      },
      "required": [
        "description"
      ]
    },
    "agent-details": {
      "type": "object",
      "properties": {
        "agent": {
          "$ref": "#/definitions/agent"
        }
      },
      "required": []
    },
    "checklist": {
