            "fascia",
            "projecting-hanging",
            "hoarding",
            "other"
          ]
        },
        "advertisement-count": {
          "type": "number"
        },
        "advertisement-other-description": {
          "type": "string"
        }
      },
      "required": [
        "advertisement-type",
        "advertisement-count"
      ],
      "if": {
        "properties": {
          "advertisement-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "advertisement-other-description"
        ]
      }
    },
    "supporting-document": {
      "type": "object",
      "properties": {
