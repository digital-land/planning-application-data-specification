        "number": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "required": [
        "fullname",
        "number",
        "email"
      ]
    },
    "advertisement": {
      "type": "object",
      "properties": {
        "height-from-ground": {
