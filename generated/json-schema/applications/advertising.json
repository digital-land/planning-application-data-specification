        "height": {
          "type": "number"
        },
        "width": {
          "type": "number"
        },
        "depth": {
          "type": "number"
        },
        "symbol-height-max": {
          "type": "number"
        },
        "colour": {
          "type": "string"
        },
        "materials": {
          "type": "string"
        },
        "max-projection": {
          "type": "number"
        },
        "illuminated": {
          "type": "boolean"
        },
        "illumination-method": {
          "type": "string"
        },
        "illuminance-level": {
          "type": "number"
        },
        "illumination-type": {
          "type": "string",
          "enum": [
            "static",
            "intermittent"
          ]
        }
      },
      "required": [],
      "allOf": [
        {
          "if": {
            "properties": {
              "illuminated": {
                "const": true
              }
            }
          },
          "then": {
            "required": [
              "illumination-method"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "illuminated": {
                "const": true
              }
            }
          },
          "then": {
            "required": [
              "illuminance-level"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "illuminated": {
                "const": true
              }
            }
          },
          "then": {
