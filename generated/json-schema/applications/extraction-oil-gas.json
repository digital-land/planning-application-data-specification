        },
        "applicant-interest": {
          "type": "string"
        },
        "applicant-interest-adjoining-land": {
          "type": "string"
        }
      },
      "required": [
        "site-owner",
        "applicant-interest",
        "applicant-interest-adjoining-land"
      ]
    },
    "conflict-of-interest": {
      "type": "object",
      "properties": {
        "conflict-to-declare": {
