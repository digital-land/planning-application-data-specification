    },
    "agent-details": {
      "$ref": "#/definitions/agent-details"
    },
    "applicant-contact": {
      "$ref": "#/definitions/applicant-contact"
    },
    "applicant-details": {
      "$ref": "#/definitions/applicant-details"
    },
    "flood-risk-assessment": {
      "$ref": "#/definitions/flood-risk-assessment"
    },
    "conflict-of-interest": {
      "$ref": "#/definitions/conflict-of-interest"
    },
    "bio-geo-arch-con": {
      "$ref": "#/definitions/bio-geo-arch-con"
    },
    "checklist": {
      "$ref": "#/definitions/checklist"
    },
    "declaration": {
      "$ref": "#/definitions/declaration"
    },
    "designated-areas": {
      "$ref": "#/definitions/designated-areas"
    },
    "employment": {
      "$ref": "#/definitions/employment"
    },
    "equip-method": {
      "$ref": "#/definitions/equip-method"
    },
    "existing-use": {
      "$ref": "#/definitions/existing-use"
    },
    "foul-sewage": {
      "$ref": "#/definitions/foul-sewage"
    },
    "haz-substances": {
      "$ref": "#/definitions/haz-substances"
    },
    "hrs-operation": {
      "$ref": "#/definitions/hrs-operation"
    },
    "ownership-certs": {
      "$ref": "#/definitions/ownership-certs"
    },
    "access-rights-of-way": {
      "$ref": "#/definitions/access-rights-of-way"
    },
    "plans-drawings-supporting-materials": {
      "$ref": "#/definitions/plans-drawings-supporting-materials"
    },
    "pre-app-advice": {
      "$ref": "#/definitions/pre-app-advice"
    },
    "site-details": {
      "$ref": "#/definitions/site-details"
    },
    "site-ownership": {
      "$ref": "#/definitions/site-ownership"
    },
    "site-visit": {
      "$ref": "#/definitions/site-visit"
    },
    "storage-facilities": {
      "$ref": "#/definitions/storage-facilities"
    },
    "trade-effluent": {
      "$ref": "#/definitions/trade-effluent"
    },
    "trees-hedges": {
      "$ref": "#/definitions/trees-hedges"
    },
    "dev-type": {
      "$ref": "#/definitions/dev-type"
    },
    "vol-agreement": {
      "$ref": "#/definitions/vol-agreement"
    },
    "application": {
      "$ref": "#/definitions/application"
    }
  },
  "required": [
    "agent-contact",
    "agent-details",
    "applicant-contact",
    "applicant-details",
    "flood-risk-assessment",
    "conflict-of-interest",
    "bio-geo-arch-con",
    "checklist",
    "declaration",
    "designated-areas",
    "employment",
    "equip-method",
    "existing-use",
    "foul-sewage",
    "haz-substances",
    "hrs-operation",
    "ownership-certs",
    "access-rights-of-way",
    "plans-drawings-supporting-materials",
    "pre-app-advice",
    "site-details",
    "site-ownership",
    "site-visit",
    "storage-facilities",
    "trade-effluent",
    "trees-hedges",
    "dev-type",
    "vol-agreement",
    "application"
  ],
  "definitions": {
    "applicant": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "notified-person": {
      "type": "object",
      "properties": {
        "person": {
          "$ref": "#/definitions/person"
        },
        "notice-date": {
          "type": "string"
        }
      },
      "required": []
    },
    "employees": {
      "type": "object",
      "properties": {
        "full-time": {
          "type": "number"
        },
        "part-time": {
          "type": "number"
        },
        "total-fte": {
          "type": "number"
        }
      },
      "required": [
        "full-time",
        "part-time",
        "total-fte"
      ]
    },
    "supporting-document": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "reference",
        "name"
      ]
    },
    "newspaper-notice": {
      "type": "object",
      "properties": {
        "newspaper-name": {
          "type": "string"
        },
        "publication-date": {
          "type": "string"
        }
      },
      "required": [
        "newspaper-name",
        "publication-date"
      ]
    },
    "person": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "first-name": {
          "type": "string"
        },
        "last-name": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        }
      },
      "required": [
        "first-name",
        "last-name",
        "address-text"
      ]
    },
    "contact-details": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "phone-numbers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/phone-number"
          },
          "minItems": 1
        }
      },
      "required": [
        "email",
        "phone-numbers"
      ]
    },
    "hours-of-operation": {
      "type": "object",
      "properties": {
        "use": {
          "type": "string",
          "enum": [
            "b2",
            "b8",
            "c1",
            "c2",
            "c2a",
            "c3",
            "c4",
            "e-a",
            "e-b",
            "e-c-i",
            "e-c-ii",
            "e-c-iii",
            "e-d",
            "e-e",
            "e-f",
            "e-g-i",
            "e-g-ii",
            "e-g-iii",
            "f1-a",
            "f1-b",
            "f1-c",
            "f1-d",
            "f1-e",
            "f1-f",
            "f1-g",
            "f2-a",
            "f2-b",
            "f2-c",
            "f2-d",
            "sui",
            "other"
          ]
        },
        "use-other": {
          "type": "string"
        },
        "operational-times": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/operational-times"
          },
          "minItems": 1
        },
        "hours-not-known": {
          "type": "boolean"
        }
      },
      "required": [
        "use"
      ],
      "allOf": [
        {
          "if": {
            "properties": {
              "use": {
                "const": "other"
              }
            }
          },
          "then": {
            "required": [
              "use-other"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "hours-not-known": {
                "const": true
              }
            }
          },
          "then": {
            "required": [
              "operational-times"
            ]
          }
        }
      ]
    },
    "operational-times": {
      "type": "object",
      "properties": {
        "day-type": {
          "type": "string",
          "enum": [
            "monday-friday",
            "saturday",
            "sunday",
            "bank-holiday"
          ]
        },
        "closed": {
          "type": "boolean"
        },
        "time-ranges": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/time-range"
          },
          "minItems": 1
        }
      },
      "required": [
        "day-type"
      ],
      "if": {
        "properties": {
          "closed": {
            "const": false
          }
        }
      },
      "then": {
        "required": [
          "time-ranges"
        ]
      }
    },
    "time-range": {
      "type": "object",
      "properties": {
        "open-time": {
          "type": "string"
        },
        "close-time": {
          "type": "string"
        }
      },
      "required": [
        "open-time",
        "close-time"
      ]
    },
    "agent": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        },
        "company": {
          "type": "string"
        },
        "user-role": {
          "type": "string",
          "enum": [
            "agent",
            "proxy"
          ]
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "site-owner": {
      "type": "object",
      "properties": {
        "fullname": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        }
      },
      "required": [
        "fullname",
        "address-text"
      ]
    },
    "existing-use-detail": {
      "type": "object",
      "properties": {
        "use": {
          "type": "string",
          "enum": [
            "b2",
            "b8",
            "c1",
            "c2",
            "c2a",
            "c3",
            "c4",
            "e-a",
            "e-b",
            "e-c-i",
            "e-c-ii",
            "e-c-iii",
            "e-d",
            "e-e",
            "e-f",
            "e-g-i",
            "e-g-ii",
            "e-g-iii",
            "f1-a",
            "f1-b",
            "f1-c",
            "f1-d",
            "f1-e",
            "f1-f",
            "f1-g",
            "f2-a",
            "f2-b",
            "f2-c",
            "f2-d",
            "sui",
            "other"
          ]
        },
        "use-details": {
          "type": "string"
        },
        "land-part": {
          "type": "string"
        }
      },
      "required": [
        "use",
        "land-part"
      ],
      "if": {
        "allOf": [
          {
            "properties": {
              "use": {
                "const": "sui"
              }
            }
          },
          {
            "properties": {
              "use": {
                "const": "other"
              }
            }
          }
        ]
      },
      "then": {
        "required": [
          "use-details"
        ]
      }
    },
    "other-contact": {
      "type": "object",
      "properties": {
        "fullname": {
          "type": "string"
        },
        "number": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "required": [
        "fullname",
        "number",
        "email"
      ]
    },
    "plans-document": {
      "type": "object",
      "properties": {
        "reference-number": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "reference-number",
        "name"
      ]
    },
    "site-location": {
      "type": "object",
      "properties": {
        "site-boundary": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        },
        "easting": {
          "type": "number"
        },
        "northing": {
          "type": "number"
        },
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "description": {
          "type": "string"
        },
        "uprns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        }
      },
      "required": []
    },
    "hazardous-substance": {
      "type": "object",
      "properties": {
        "hazardous-substance-type": {
          "type": "string",
          "enum": [
            "acrylonitrile",
            "ammonia",
            "bromine",
            "chlorine",
            "ethylene-oxide",
            "flour",
            "hydrogen-cyanide",
            "liquid-oxygen",
            "liquid-petroleum-gas",
            "phosgene",
            "refined-white-sugar",
            "sulphur-dioxide"
          ]
        },
        "hazardous-substance-other": {
          "type": "string"
        },
        "amount": {
          "type": "number"
        }
      },
      "required": [
        "hazardous-substance-type",
        "amount"
      ],
      "if": {
        "properties": {
          "hazardous-substance-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "hazardous-substance-other"
        ]
      }
    },
    "phone-number": {
      "type": "object",
      "properties": {
        "number": {
          "type": "string"
        },
        "contact-priority": {
          "type": "string",
          "enum": [
            "primary",
            "secondary"
          ]
        }
      },
      "required": []
    },
    "dev-type": {
      "type": "object",
      "properties": {
        "development-phase": {
          "type": "string",
          "enum": [
            "exploratory",
            "appraisal",
            "production"
          ]
        },
        "development-description": {
          "type": "string"
        },
        "quantity-cubic-metres": {
          "type": "number"
        },
        "permission-period-years": {
          "type": "number"
        },
        "hydrocarbon-licence-block": {
          "type": "string"
        },
        "surface-site-area-hectares": {
          "type": "number"
        },
        "site-hectares-provided-by": {
          "type": "string",
          "enum": [
            "applicant",
            "system"
          ]
        },
        "environmental-statement": {
          "type": "boolean"
        },
        "environmental-statement-reference": {
          "type": "string"
        }
      },
      "required": [
        "development-phase",
        "development-description",
        "quantity-cubic-metres",
        "hydrocarbon-licence-block",
        "environmental-statement"
      ],
      "if": {
        "properties": {
          "environmental-statement": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "environmental-statement-reference"
        ]
      }
    },
    "employment": {
      "type": "object",
      "properties": {
        "existing-employees": {
          "$ref": "#/definitions/employees"
        },
        "proposed-employees": {
          "$ref": "#/definitions/employees"
        },
        "employment-impact": {
          "type": "string"
        }
      },
      "required": [
        "existing-employees",
        "proposed-employees"
      ]
    },
    "flood-risk-assessment": {
      "type": "object",
      "properties": {
        "flood-risk-area": {
          "type": "boolean"
        },
        "data-provided-by": {
          "type": "string",
          "enum": [
            "applicant",
            "system"
          ]
        },
        "flood-risk-assessment": {
          "type": "string"
        },
        "within-20m-watercourse": {
          "type": "boolean"
        },
        "increases-flood-risk": {
          "type": "boolean"
        },
        "surface-water-disposal": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "sustainable-drainage",
              "soakaway",
              "main-sewer",
              "existing-watercourse",
              "pond-lake"
            ]
          },
          "minItems": 1
        }
      },
      "required": [
        "flood-risk-area",
        "within-20m-watercourse",
        "increases-flood-risk",
        "surface-water-disposal"
      ],
      "if": {
        "properties": {
          "flood-risk-area": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "flood-risk-assessment"
        ]
      }
    },
    "applicant-details": {
      "type": "object",
      "properties": {
        "applicants": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/applicant"
          },
          "minItems": 1
        }
      },
      "required": [
        "applicants"
      ]
    },
    "trees-hedges": {
      "type": "object",
      "properties": {
        "trees-on-site": {
          "type": "boolean"
        },
        "trees-on-adj-land": {
          "type": "boolean"
        }
      },
      "required": [
        "trees-on-site",
        "trees-on-adj-land"
      ]
    },
    "site-details": {
      "type": "object",
      "properties": {
        "site-locations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/site-location"
          },
          "minItems": 1
        }
      },
      "required": [
        "site-locations"
      ]
    },
    "site-visit": {
      "type": "object",
      "properties": {
        "can-be-seen-from": {
          "type": "boolean"
        },
        "contact-type": {
          "type": "string",
          "enum": [
            "applicant",
            "agent"
          ]
        },
        "contact-reference": {
          "type": "string"
        },
        "other-contact": {
          "$ref": "#/definitions/other-contact"
        }
      },
      "required": [
        "can-be-seen-from",
        "contact-type"
      ],
      "if": {
        "properties": {
          "contact-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "other-contact"
        ]
      }
    },
    "access-rights-of-way": {
      "type": "object",
      "properties": {
        "new-altered-vehicle": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "new-altered-pedestrian": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "new-right-of-way": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "new-public-road": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "temp-right-of-way": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "future-new-right-of-way": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "supporting-documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/supporting-document"
          },
          "minItems": 1
        }
      },
      "required": [
        "new-altered-vehicle",
        "new-altered-pedestrian",
        "new-right-of-way",
        "new-public-road",
        "temp-right-of-way",
        "future-new-right-of-way"
      ],
      "if": {
        "anyOf": [
          {
            "properties": {
              "new-altered-vehicle": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "new-altered-pedestrian": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "change-right-of-way": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "new-right-of-way": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "new-public-road": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "temp-right-of-way": {
                "const": "true"
              }
            }
          },
          {
            "properties": {
              "future-new-right-of-way": {
                "const": "true"
              }
            }
          }
        ]
      },
      "then": {
        "required": [
          "supporting-documents"
        ]
      }
    },
    "vol-agreement": {
      "type": "object",
      "properties": {
        "draft-agreement-included": {
          "type": "boolean"
        },
        "agreement-summary": {
          "type": "string"
        }
      },
      "required": [
        "draft-agreement-included"
      ],
      "if": {
        "properties": {
          "draft-agreement-included": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "agreement-summary"
        ]
      }
    },
