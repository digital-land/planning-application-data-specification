{
  "$schema": "https://json-schema.org/draft-07/schema#",
  "$id": "technical-details-consent.json",
  "title": "Technical details consent",
  "description": "Technical Details Consent (TDC) is the second stage of the 'Permission in Principle' (PiP) process in planning, primarily for housing-led development. It follows the initial 'Permission in Principle' stage, which establishes whether a site is suitable in principle for development. TDC assesses the detailed design, layout, and other technical aspects of the proposed development.\n",
  "type": "object",
  "properties": {
    "access-rights-of-way": {
      "$ref": "#/definitions/access-rights-of-way"
    },
    "agent-contact": {
      "$ref": "#/definitions/agent-contact"
    },
    "agent-details": {
      "$ref": "#/definitions/agent-details"
    },
    "applicant-contact": {
      "$ref": "#/definitions/applicant-contact"
    },
    "applicant-details": {
      "$ref": "#/definitions/applicant-details"
    },
    "bio-geo-arch-con": {
      "$ref": "#/definitions/bio-geo-arch-con"
    },
    "bng": {
      "$ref": "#/definitions/bng"
    },
    "checklist": {
      "$ref": "#/definitions/checklist"
    },
    "conflict-of-interest": {
      "$ref": "#/definitions/conflict-of-interest"
    },
    "declaration": {
      "$ref": "#/definitions/declaration"
    },
    "employment": {
      "$ref": "#/definitions/employment"
    },
    "existing-use": {
      "$ref": "#/definitions/existing-use"
    },
    "flood-risk-assessment": {
      "$ref": "#/definitions/flood-risk-assessment"
    },
    "foul-sewage": {
      "$ref": "#/definitions/foul-sewage"
    },
    "haz-substances": {
      "$ref": "#/definitions/haz-substances"
    },
    "hrs-operation": {
      "$ref": "#/definitions/hrs-operation"
    },
    "materials": {
      "$ref": "#/definitions/materials"
    },
    "non-res-floorspace": {
      "$ref": "#/definitions/non-res-floorspace"
    },
    "ownership-certs": {
      "$ref": "#/definitions/ownership-certs"
    },
    "pre-app-advice": {
      "$ref": "#/definitions/pre-app-advice"
    },
    "processes-machinery-waste": {
      "$ref": "#/definitions/processes-machinery-waste"
    },
    "proposal-details": {
      "$ref": "#/definitions/proposal-details"
    },
    "res-units": {
      "$ref": "#/definitions/res-units"
    },
    "site-area": {
      "$ref": "#/definitions/site-area"
    },
    "site-details": {
      "$ref": "#/definitions/site-details"
    },
    "site-visit": {
      "$ref": "#/definitions/site-visit"
    },
    "trade-effluent": {
      "$ref": "#/definitions/trade-effluent"
    },
    "trees-hedges": {
      "$ref": "#/definitions/trees-hedges"
    },
    "vehicle-parking": {
      "$ref": "#/definitions/vehicle-parking"
    },
    "waste-storage-collection": {
      "$ref": "#/definitions/waste-storage-collection"
    },
    "application": {
      "$ref": "#/definitions/application"
    }
  },
  "required": [
    "access-rights-of-way",
    "agent-contact",
    "agent-details",
    "applicant-contact",
    "applicant-details",
    "bio-geo-arch-con",
    "bng",
    "checklist",
    "conflict-of-interest",
    "declaration",
    "employment",
    "existing-use",
    "flood-risk-assessment",
    "foul-sewage",
    "haz-substances",
    "hrs-operation",
    "materials",
    "non-res-floorspace",
    "ownership-certs",
    "pre-app-advice",
    "processes-machinery-waste",
    "proposal-details",
    "res-units",
    "site-area",
    "site-details",
    "site-visit",
    "trade-effluent",
    "trees-hedges",
    "vehicle-parking",
    "waste-storage-collection",
    "application"
  ],
  "definitions": {
    "site-location": {
      "type": "object",
      "properties": {
        "site-boundary": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        },
        "easting": {
          "type": "number"
        },
        "northing": {
          "type": "number"
        },
        "latitude": {
          "type": "number"
        },
        "longitude": {
          "type": "number"
        },
        "description": {
          "type": "string"
        },
        "uprns": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        }
      },
      "required": []
    },
    "existing-use-detail": {
      "type": "object",
      "properties": {
        "use": {
          "type": "string",
          "enum": [
            "b2",
            "b8",
            "c1",
            "c2",
            "c2a",
            "c3",
            "c4",
            "e-a",
            "e-b",
            "e-c-i",
            "e-c-ii",
            "e-c-iii",
            "e-d",
            "e-e",
            "e-f",
            "e-g-i",
            "e-g-ii",
            "e-g-iii",
            "f1-a",
            "f1-b",
            "f1-c",
            "f1-d",
            "f1-e",
            "f1-f",
            "f1-g",
            "f2-a",
            "f2-b",
            "f2-c",
            "f2-d",
            "sui",
            "other"
          ]
        },
        "use-details": {
          "type": "string"
        },
        "land-part": {
          "type": "string"
        }
      },
      "required": [
        "use",
        "land-part"
      ],
      "if": {
        "allOf": [
          {
            "properties": {
              "use": {
                "const": "sui"
              }
            }
          },
          {
            "properties": {
              "use": {
                "const": "other"
              }
            }
          }
        ]
      },
      "then": {
        "required": [
          "use-details"
        ]
      }
    },
    "parking-space": {
      "type": "object",
      "properties": {
        "parking-space-type": {
          "type": "string",
          "enum": [
            "car-space",
            "light-goods-vehicle-space",
            "motorcycle-space",
            "disability-space",
            "cycle-space",
            "blue-badge-space",
            "bus",
            "car-club",
            "resi-off-street",
            "other"
          ]
        },
        "vehicle-type-other": {
          "type": "string"
        },
        "total-existing": {
          "type": "number"
        },
        "total-proposed": {
          "type": "number"
        },
        "difference-in-spaces": {
          "type": "number"
        }
      },
      "required": [
        "parking-space-type",
        "total-existing",
        "total-proposed",
        "difference-in-spaces"
      ],
      "if": {
        "properties": {
          "parking-space-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "vehicle-type-other"
        ]
      }
    },
    "waste-streams": {
      "type": "object",
      "properties": {
        "municipal": {
          "type": "number"
        },
        "construction-demolition": {
          "type": "number"
        },
        "commercial-industrial": {
          "type": "number"
        },
        "hazardous": {
          "type": "number"
        }
      },
      "required": []
    },
    "waste-management": {
      "type": "object",
      "properties": {
        "waste-management-facility-type": {
          "type": "string",
          "enum": [
            "inert-landfill",
            "non-hazardous-landfill",
            "hazardous-landfill",
            "energy-waste-incineration",
            "other-incineration",
            "landfill-gas-plant",
            "pyrolysis-gasification",
            "metal-recycling",
            "transfer-stations",
            "mrf",
            "household-amenity-site",
            "open-windrow-composting",
            "in-vessel-composting",
            "anaerobic-digestion",
            "mbt",
            "sewage-treatment",
            "other-treatment",
            "construction-recycling",
            "waste-storage",
            "other-waste-management",
            "other-developments"
          ]
        },
        "not-applicable": {
          "type": "boolean"
        },
        "total-capacity": {
          "type": "number"
        },
        "annual-throughput": {
          "type": "number"
        }
      },
      "required": [
        "waste-management-facility-type"
      ],
      "allOf": [
        {
          "if": {
            "properties": {
              "not-applicable": {
                "const": false
              }
            }
          },
          "then": {
            "required": [
              "total-capacity"
            ]
          }
        },
        {
          "if": {
            "properties": {
              "not-applicable": {
                "const": false
              }
            }
          },
          "then": {
            "required": [
              "annual-throughput"
            ]
          }
        }
      ]
    },
    "building-element": {
      "type": "object",
      "properties": {
        "building-element-type": {
          "type": "string",
          "enum": [
            "walls",
            "roof",
            "windows",
            "doors",
            "boundary-treatments",
            "vehicle-access-hard-standings",
            "lighting",
            "external-walls",
            "roof-covering",
            "chimney",
            "external-doors",
            "ceilings",
            "internal-walls",
            "floors",
            "internal-doors",
            "rainwater-goods",
            "other"
          ]
        },
        "existing-materials": {
          "type": "string"
        },
        "proposed-materials": {
          "type": "string"
        },
        "materials-not-applicable": {
          "type": "boolean"
        },
        "materials-not-known": {
          "type": "boolean"
        }
      },
      "required": [
        "building-element-type"
      ]
    },
    "applicant": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "person": {
      "type": "object",
      "properties": {
        "title": {
          "type": "string"
        },
        "first-name": {
          "type": "string"
        },
        "last-name": {
          "type": "string"
        },
        "address-text": {
          "type": "string"
        },
        "postcode": {
          "type": "string"
        }
      },
      "required": [
        "first-name",
        "last-name",
        "address-text"
      ]
    },
    "supporting-document": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "name": {
          "type": "string"
        }
      },
      "required": [
        "reference",
        "name"
      ]
    },
    "agent": {
      "type": "object",
      "properties": {
        "reference": {
          "type": "string"
        },
        "person": {
          "$ref": "#/definitions/person"
        },
        "company": {
          "type": "string"
        },
        "user-role": {
          "type": "string",
          "enum": [
            "agent",
            "proxy"
          ]
        }
      },
      "required": [
        "reference",
        "person"
      ]
    },
    "contact-details": {
      "type": "object",
      "properties": {
        "email": {
          "type": "string"
        },
        "phone-numbers": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/phone-number"
          },
          "minItems": 1
        }
      },
      "required": [
        "email",
        "phone-numbers"
      ]
    },
    "employees": {
      "type": "object",
      "properties": {
        "full-time": {
          "type": "number"
        },
        "part-time": {
          "type": "number"
        },
        "total-fte": {
          "type": "number"
        }
      },
      "required": [
        "full-time",
        "part-time",
        "total-fte"
      ]
    },
    "phone-number": {
      "type": "object",
      "properties": {
        "number": {
          "type": "string"
        },
        "contact-priority": {
          "type": "string",
          "enum": [
            "primary",
            "secondary"
          ]
        }
      },
      "required": []
    },
    "residential-unit-summary": {
      "type": "object",
      "properties": {
        "tenure-type": {
          "type": "string",
          "enum": [
            "market-housing",
            "social-rented",
            "intermediate-housing",
            "key-worker-housing",
            "affordable-rent",
            "home-ownership",
            "starter-homes",
            "custom-build"
          ]
        },
        "housing-type": {
          "type": "string",
          "enum": [
            "houses",
            "flats-maisonettes",
            "sheltered-housing",
            "bedsit-studio",
            "cluster-flats",
            "other",
            "live-work-units",
            "unknown"
          ]
        },
        "existing-unit-breakdown": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/unit-quantities"
          },
          "minItems": 1
        },
        "proposed-unit-breakdown": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/unit-quantities"
          },
          "minItems": 1
        }
      },
      "required": [
        "tenure-type",
        "housing-type"
      ]
    },
    "hazardous-substance": {
      "type": "object",
      "properties": {
        "hazardous-substance-type": {
          "type": "string",
          "enum": [
            "acrylonitrile",
            "ammonia",
            "bromine",
            "chlorine",
            "ethylene-oxide",
            "flour",
            "hydrogen-cyanide",
            "liquid-oxygen",
            "liquid-petroleum-gas",
            "phosgene",
            "refined-white-sugar",
            "sulphur-dioxide"
          ]
        },
        "hazardous-substance-other": {
          "type": "string"
        },
        "amount": {
          "type": "number"
        }
      },
      "required": [
        "hazardous-substance-type",
        "amount"
      ],
      "if": {
        "properties": {
          "hazardous-substance-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "hazardous-substance-other"
        ]
      }
    },
    "other-contact": {
      "type": "object",
      "properties": {
        "fullname": {
          "type": "string"
        },
        "number": {
          "type": "string"
        },
        "email": {
          "type": "string"
        }
      },
      "required": [
        "fullname",
        "number",
        "email"
      ]
    },
    "unit-quantities": {
      "type": "object",
      "properties": {
        "units-unknown": {
          "type": "boolean"
        },
        "units-per-bedroom-no": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/bedroom-count"
          },
          "minItems": 1
        },
        "total-units": {
          "type": "number"
        }
      },
      "required": [
        "units-unknown"
      ],
      "if": {
        "properties": {
          "units-unknown": {
            "const": false
          }
        }
      },
      "then": {
        "required": [
          "units-per-bedroom-no"
        ]
      }
    },
    "bedroom-count": {
      "type": "object",
      "properties": {
        "no-bedrooms-unknown": {
          "type": "boolean"
        },
        "no-of-bedrooms": {
          "type": "number"
        },
        "units": {
          "type": "number"
        }
      },
      "required": [
        "no-bedrooms-unknown",
        "units"
      ],
      "if": {
        "properties": {
          "no-bedrooms-unknown": {
            "const": false
          }
        }
      },
      "then": {
        "required": [
          "no-of-bedrooms"
        ]
      }
    },
    "trees-hedges": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "agent-contact": {
      "type": "object",
      "properties": {
        "agent-reference": {
          "type": "string"
        },
        "contact-details": {
          "$ref": "#/definitions/contact-details"
        }
      },
      "required": [
        "agent-reference",
        "contact-details"
      ]
    },
    "ownership-certs": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "vehicle-parking": {
      "type": "object",
      "properties": {
        "parking-spaces": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/parking-space"
          },
          "minItems": 1
        }
      },
      "required": [
        "parking-spaces"
      ]
    },
    "bng": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "flood-risk-assessment": {
      "type": "object",
      "properties": {
        "flood-risk-area": {
          "type": "boolean"
        },
        "data-provided-by": {
          "type": "string",
          "enum": [
            "applicant",
            "system"
          ]
        },
        "flood-risk-assessment": {
          "type": "string"
        },
        "within-20m-watercourse": {
          "type": "boolean"
        },
        "increases-flood-risk": {
          "type": "boolean"
        },
        "surface-water-disposal": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "sustainable-drainage",
              "soakaway",
              "main-sewer",
              "existing-watercourse",
              "pond-lake"
            ]
          },
          "minItems": 1
        }
      },
      "required": [
        "flood-risk-area",
        "within-20m-watercourse",
        "increases-flood-risk",
        "surface-water-disposal"
      ],
      "if": {
        "properties": {
          "flood-risk-area": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "flood-risk-assessment"
        ]
      }
    },
    "site-details": {
      "type": "object",
      "properties": {
        "site-locations": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/site-location"
          },
          "minItems": 1
        }
      },
      "required": [
        "site-locations"
      ]
    },
    "employment": {
      "type": "object",
      "properties": {
        "existing-employees": {
          "$ref": "#/definitions/employees"
        },
        "proposed-employees": {
          "$ref": "#/definitions/employees"
        },
        "employment-impact": {
          "type": "string"
        }
      },
      "required": [
        "existing-employees",
        "proposed-employees"
      ]
    },
    "res-units": {
      "type": "object",
      "properties": {
        "will-residential-units-change": {
          "type": "boolean"
        },
        "residential-unit-summary": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/residential-unit-summary"
          },
          "minItems": 1
        },
        "total-existing-units": {
          "type": "number"
        },
        "total-proposed-units": {
          "type": "number"
        },
        "net-change": {
          "type": "number"
        }
      },
      "required": [
        "will-residential-units-change",
        "total-existing-units",
        "total-proposed-units",
        "net-change"
      ],
      "if": {
        "properties": {
          "will-residential-units-change": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "residential-unit-summary"
        ]
      }
    },
    "site-visit": {
      "type": "object",
      "properties": {
        "can-be-seen-from": {
          "type": "boolean"
        },
        "contact-type": {
          "type": "string",
          "enum": [
            "applicant",
            "agent"
          ]
        },
        "contact-reference": {
          "type": "string"
        },
        "other-contact": {
          "$ref": "#/definitions/other-contact"
        }
      },
      "required": [
        "can-be-seen-from",
        "contact-type"
      ],
      "if": {
        "properties": {
          "contact-type": {
            "const": "other"
          }
        }
      },
      "then": {
        "required": [
          "other-contact"
        ]
      }
    },
    "applicant-contact": {
      "type": "object",
      "properties": {
        "applicant-reference": {
          "type": "string"
        },
        "contact-details": {
          "$ref": "#/definitions/contact-details"
        }
      },
      "required": [
        "applicant-reference",
        "contact-details"
      ]
    },
    "foul-sewage": {
      "type": "object",
      "properties": {
        "has-new-disposal-arrangements": {
          "type": "boolean"
        },
        "foul-sewage-disposal-types": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": [
              "mains-sewer",
              "cess-pit",
              "septic-tank",
              "package-treatment",
              "other"
            ]
          },
          "minItems": 1
        },
        "produce-foul-sewage": {
          "type": "boolean"
        },
        "connect-to-drainage-system": {
          "type": "boolean"
        },
        "drainage-system-details": {
          "type": "string"
        }
      },
      "required": [
        "has-new-disposal-arrangements",
        "produce-foul-sewage",
        "connect-to-drainage-system"
      ],
      "if": {
        "properties": {
          "has-new-disposal-arrangements": {
            "const": true
          }
        }
      },
      "then": {
        "required": [
          "foul-sewage-disposal-types"
        ]
      }
    },
    "conflict-of-interest": {
      "type": "object",
      "properties": {},
      "required": []
    },
    "access-rights-of-way": {
      "type": "object",
      "properties": {
        "new-altered-vehicle": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "new-altered-pedestrian": {
          "type": "string",
          "enum": [
            "true",
            "false",
            "unknown"
          ]
        },
        "supporting-documents": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/supporting-document"
          },
          "minItems": 1
